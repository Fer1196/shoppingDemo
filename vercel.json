{
  "rewrites": [

       {"source": "/(.*)", "destination": "/"},

    {
  "source": "/((?!notroute).*)",
  "destination": "/notroute"
},

     {
  "source": "/((?!shopping).*)",
  "destination": "/shopping"
},

        {
  "source": "/((?!cart).*)",
  "destination": "/cart"
},

    {
  "source": "/product/:type",
  "destination": "/feedback/:id"
}
    

  



  ]
}
